{
  "connection": "mongo",
  "collectionName": "contacts",
  "info": {
    "name": "contact",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "link": {
      "type": "string"
    },
    "type": {
      "enum": [
        "other",
        "email",
        "pinterest",
        "linkedin",
        "git",
        "xing",
        "facebook"
      ],
      "type": "enumeration",
      "required": true
    },
    "name": {
      "type": "string"
    }
  }
}
